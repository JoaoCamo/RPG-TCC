{
    "format":{
        "type":"json_schema",
        "name":"character",
        "strict":true,
        "schema":{
            "type":"object",
            "properties":{
                "level":{
                    "type":"integer",
                    "minimum":1
                },
                "name":{
                    "type":"string"
                },
                "stats":{
                    "type":"object",
                    "properties":{
                        "strength":{
                            "type":"integer"
                        },
                        "dexterity":{
                            "type":"integer"
                        },
                        "constitution":{
                            "type":"integer"
                        },
                        "intelligence":{
                            "type":"integer"
                        },
                        "wisdom":{
                            "type":"integer"
                        },
                        "charisma":{
                            "type":"integer"
                        }
                    },
                    "required":[
                        "strength",
                        "dexterity",
                        "constitution",
                        "intelligence",
                        "wisdom",
                        "charisma"
                    ],
                    "additionalProperties":false
                },
                "inventory":{
                    "type":"array",
                    "items":{
                        "type":"string",
                        "uniqueItems":true
                    }
                }
            },
            "required":[
                "level",
                "name",
                "stats",
                "inventory"
            ],
            "additionalProperties":false
        }
    }
}