{
    "format":{
        "type":"json_schema",
        "name":"main_story",
        "strict":true,
        "schema":{
            "type":"object",
            "properties":{
                "kingdom":{
                    "type":"object",
                    "properties":{
                        "name":{
                            "type":"string"
                        },
                        "description":{
                            "type":"string"
                        }
                    },
                    "required":[
                        "name",
                        "description"
                    ],
                    "additionalProperties":false
                },
                "dungeon":{
                    "type":"object",
                    "properties":{
                        "name":{
                            "type":"string"
                        },
                        "description":{
                            "type":"string"
                        }
                    },
                    "required":[
                        "name",
                        "description"
                    ],
                    "additionalProperties":false
                },
                "introduction":{
                    "type":"string"
                }
            },
            "required":[
                "kingdom",
                "dungeon",
                "introduction"
            ],
            "additionalProperties":false
        }
    }
}